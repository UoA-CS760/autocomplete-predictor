    "from statistics import mean\n",
    "\n",
    "with open('token-seq') as f:\n",
    "    tokensPerLine  = []\n",
    "    for file in f:\n",
    "        for line in file.split('<new_line>'):\n",
    "            tokensPerLine.append(len(line))\n",
    "\n",
    "mean(tokensPerLine)"
   ]
